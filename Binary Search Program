#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, key;

    cout << "Enter number of elements: ";
    cin >> n;

    vector<int> a(n);

    cout << "Enter sorted elements:\n";
    for(int i = 0; i < n; i++) {
        cin >> a[i];
    }

    cout << "Enter element to search: ";
    cin >> key;

    int low = 0, high = n - 1;
    bool found = false;

    while(low <= high) {
        int mid = low + (high - low) / 2;

        if(a[mid] == key) {
            cout << "Element found at position " << mid + 1;
            found = true;
            break;
        }
        else if(a[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }

    if(!found)
        cout << "Element not found";

    return 0;
}
